    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-infinite-scroller": "^1.2.4",
    "react-virtualized": "^9.21.2",
    "umi": "^3.1.1",
    "umi-plugin-keep-alive": "^0.0.1-beta.9",
    "yorkie": "^2.0.0"
